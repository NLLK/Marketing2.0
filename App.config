<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="StartupFolders" type="MegaMarketing2Reborn.SettingsSetup.StartupFoldersConfigSection, MegaMarketing2Reborn" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
